{
    "manifest_version": 3,
    "name": "Buck-athon",
    "description": "Base Level Extension",
    "version": "1.0",
    "author": "Albert Hu & Daniel Song",
    "action": {
      "default_popup": "popup/popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["activeTab"]
  }